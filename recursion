def factorial(n):
    if n == 1:
        return 1
    else:
        return n*factorial(n-1)


#def factorial(n):
#    result = n
#    for i in range(1,n):
#        result +=i
#    return result

number = int(input("enter the number:"))
result = factorial(number)
print("%d factorial isï¼š%d" %(number,result))
